package br.com.healthtrack.entities;

public class Usuario {
	
	private String nome;
	private Integer idade;
	private Double peso;
	
	public Usuario(String nome, Integer idade, Double peso) {
		this.nome = nome;
		this.idade = idade;
		this.peso = peso;
	}

	@Override
	public String toString() {
		return "Usuario [nome=" + nome + ", idade=" + idade + ", peso=" + peso + "]";
	}
	
	
	

}
